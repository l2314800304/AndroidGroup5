<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.androidgroup5.onlinecontact">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CALL_LOG" />
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:name=".UserParameter"
        android:icon="@drawable/onlinecontact_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/onlinecontact_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SyncAddressBook" />
        <activity android:name=".Register" />
        <activity android:name=".QRCode.QRCodeActivity" />
        <activity android:name=".Birthday" />
        <activity android:name=".EditContactDetailActivity" />
        <activity android:name=".Delete" />
        <activity android:name=".Export" />
        <activity android:name=".Find" />
        <activity android:name=".Insert" />
        <activity android:name=".Phone" />
        <activity android:name=".Remark" />
        <activity android:name=".ShowContactActivity"
            tools:ignore="DuplicateActivity" />
        <activity android:name=".ContactDetailActivity" />
        <activity android:name=".CallLogActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".GuidePageActivity" />
        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />
    </application>
</manifest>